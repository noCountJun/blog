package org.anse.vo;

public class PagingVO {
	private String ctgId;
	private String ctgNm;
	private String ctgDepth;
	private String upCtgId;
	private String dispYn;
	private int sortNo;
	private String regDt;
	private String modDt;
	
	
	public String getCtgId() {
		return ctgId;
	}
	public void setCtgId(String ctgId) {
		this.ctgId = ctgId;
	}
	public String getCtgNm() {
		return ctgNm;
	}
	public void setCtgNm(String ctgNm) {
		this.ctgNm = ctgNm;
	}
	public String getCtgDepth() {
		return ctgDepth;
	}
	public void setCtgDepth(String ctgDepth) {
		this.ctgDepth = ctgDepth;
	}
	public String getUpCtgId() {
		return upCtgId;
	}
	public void setUpCtgId(String upCtgId) {
		this.upCtgId = upCtgId;
	}
	public String getDispYn() {
		return dispYn;
	}
	public void setDispYn(String dispYn) {
		this.dispYn = dispYn;
	}
	public int getSortNo() {
		return sortNo;
	}
	public void setSortNo(int sortNo) {
		this.sortNo = sortNo;
	}
	public String getRegDt() {
		return regDt;
	}
	public void setRegDt(String regDt) {
		this.regDt = regDt;
	}
	public String getModDt() {
		return modDt;
	}
	public void setModDt(String modDt) {
		this.modDt = modDt;
	}
	
}
